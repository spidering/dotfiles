syntax enable 
"colorscheme elflord
let g:hybrid_use_Xresources = 1
colorscheme hybrid 
"colorscheme molokai
highlight ZenkakuSpace cterm=underline ctermfg=lightblue guibg=darkgray
match ZenkakuSpace / /
set background=dark
set number
set nobackup
set encoding=utf-8
set fileencoding=utf-8
set fileencodings=iso-2022-jp,euc-jp,utf-8,ucs-2,cp932,sjis
set autoindent
set incsearch
set wrapscan
set ignorecase
set showmatch
set showmode
set title
set ruler
set tabstop=4
